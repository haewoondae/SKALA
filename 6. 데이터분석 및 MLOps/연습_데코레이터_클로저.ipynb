    "def my_decorator(func):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        print(\"함수 실행 전\")\n",
    "        result = func(*args, **kwargs)\n",
    "        return result\n",
    "\n",
    "        print(\"함수 실행 후\")\n",
    "    return wrapper\n",
    "\n",
    "@my_decorator\n",
    "def add(a, b):\n",
    "    return a + b\n",
    "\n",
    "print(add(3, 5))"
   ]
  }
